{
  "name": "gitrewind",
  "displayName": "GitRewind",
  "description": "ðŸ•°ï¸ Visualize how your code evolved through Git history with beautiful side-by-side diffs and change navigation",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onCommand:GitRewind.showHistory",
    "onView:gitrewind-graph-view"
  ],
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "history",
    "diff",
    "version-control",
    "code-evolution"
  ],
  "publisher": "vineer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nagavineerpasam/git-rewind.git"
  },
  "icon": "images/icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitrewind-explorer",
          "title": "GitRewind",
          "icon": "$(history)"
        }
      ]
    },
    "views": {
      "gitrewind-explorer": [
        {
          "id": "gitrewind-graph-view",
          "name": "Repository Graph",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "GitRewind.showHistory",
        "title": "GitRewind: Show Repository Graph"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "GitRewind.showHistory",
          "when": "workspaceFolderCount > 0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/diff": "^7.0.2",
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.109.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/parser": "^7.22.16",
    "@babel/traverse": "^7.22.15",
    "@babel/types": "^7.22.15",
    "diff": "^5.1.0",
    "highlight.js": "^11.8.0",
    "simple-git": "^3.19.1"
  }
}
